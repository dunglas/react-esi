{
  "name": "esi-next",
  "version": "1.0.0",
  "private": true,
  "description": "An example project to demonstrate usage of ESI with Next.js and Express.js",
  "repository": "https://github.com/dunglas/react-esi/examples/next",
  "license": "MIT",
  "author": "KÃ©vin Dunglas",
  "main": "dist/server.js",
  "files": [
    "dist",
    ".next",
    "components",
    "public"
  ],
  "scripts": {
    "build-next": "NODE_ENV=production next build",
    "build-server": "tsc --project tsconfig.server.json",
    "build-client": "tsc --project tsconfig.client.json",
    "build": "pnpm build-next && pnpm build-server",
    "dev": "tsx watch --tsconfig ./tsconfig.server.json  ./server.tsx",
    "start": "cross-env NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "express": "^4.18.3",
    "next": "^14.1.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-esi": "workspace:^"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "@types/prop-types": "^15.7.11",
    "@types/react": "18.2.64",
    "@types/react-dom": "^18.2.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
