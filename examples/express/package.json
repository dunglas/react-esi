{
  "name": "esi-express",
  "version": "1.0.0",
  "private": true,
  "description": "An example project to demonstrate usage of ESI with Express",
  "repository": "https://github.com/dunglas/react-esi/examples/express",
  "license": "MIT",
  "author": "KÃ©vin Dunglas",
  "main": "dist/server.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist/",
    "build:client": "node esbuild.config.mjs",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "pnpm clean && pnpm build:client && pnpm build:server",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "dev": "tsx src/server.tsx"
  },
  "dependencies": {
    "express": "^4.18.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-esi": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "@types/prop-types": "^15.7.11",
    "@types/react": "18.2.64",
    "@types/react-dom": "^18.2.21",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
